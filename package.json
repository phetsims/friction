{
  "name": "friction",
  "version": "1.6.0-dev.18",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/phetsims/friction.git"
  },
  "devDependencies": {
    "grunt": "~1.4.0"
